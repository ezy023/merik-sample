-provide(:title, 'All users')
-@user = current_user
%h1 All users
=form_tag users_path, method: 'get', class: "form-search" do
	=text_field_tag :search, params[:search], class: "input-medium search-query"
	=submit_tag "Search", name: nil, class: "btn btn-mini"
=will_paginate
%ul.users
	=render @users
=will_paginate