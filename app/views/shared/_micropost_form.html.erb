<div id="micropost-form">
<button id="song-upload-form-title">Upload a Song</button>
<div id="micropost-form-fields" class="well well-small">
<%= form_for @micropost, :html => {:multipart => true, class: "center"} do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="song-upload-fields">
  	<%= f.label :song, class: "control-label" %>
  	<%= f.file_field :song %>

 
    <%= f.text_field :title, placeholder: "Song Title" %>
    <%= f.text_field :artist, placeholder: "Artist" %>
    <%= f.text_field :genre, placeholder: "Genre" %>
  <%= f.submit "Upload", class: "btn" %>
</div>
<% end %>
</div>
</div>